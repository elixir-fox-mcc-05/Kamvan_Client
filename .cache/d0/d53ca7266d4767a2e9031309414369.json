readyLogin(){\r\n            this.isLogin=true         \r\n        },\r\n        alreadyLogout(){\r\n            this.isLogin=false\r\n        }\r\n    },\r\n    created(){\r\n        if(localStorage.token){\r\n            this.fetchTask()\r\n            this.alreadyLogin()\r\n        }\r\n        else {\r\n            this.alreadyLogout()\r\n            this.isLogin = false\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"},"lineCount":null}},"error":null,"hash":"485aa0c00ee4794bd35a8381b7e4f26d","cacheData":{"env":{}}}